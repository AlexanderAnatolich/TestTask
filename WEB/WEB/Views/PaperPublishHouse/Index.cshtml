@model IEnumerable<Model.Models.PaperPublishHouseViewModel>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PublishHouse)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PublishHouse)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}
</table>
<input type="button" value="Add publish house" onclick="location.href='@Url.Action("Create")'" />
